<Window x:Class="ChristmasGiftClient.Controller.SelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChristmasGiftClient.Controller"
        mc:Ignorable="d"
        Title="SelectWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Controller/images/background1.jpg"/>
        </Grid.Background>
        <Button x:Name="btnSubmitEmail" Content="DynamicResource btnSubmitEmail" HorizontalAlignment="Center"  Background="DarkGreen" Foreground="White" BorderBrush="{x:Null}" Click="btnSubmitEmail_Click" Height="NaN" Margin="0,258,0,141"/>
        <TextBox x:Name="textBoxEmail" Height="NaN" Margin="256,195,256,195" Width="287" FontSize="16" />
        
    </Grid>
</Window>
