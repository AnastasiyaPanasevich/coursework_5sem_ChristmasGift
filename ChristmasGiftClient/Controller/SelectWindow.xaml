<Window x:Class="ChristmasGiftClient.Controller.SelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChristmasGiftClient.Controller"
        mc:Ignorable="d"
        Title="SelectWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Controller/images/background1.jpg"/>
        </Grid.Background>

        <Label x:Name="lblChooseRandomOrCustom"  Content="{DynamicResource m_lblChooseRandomOrCustom}"  Foreground="White" FontWeight="Bold" Height="NaN" Margin="234,193,234,213" Width="NaN" />

        <Button  x:Name="btnCreateCustom"   Content="{DynamicResource m_btnCreateCustom}"  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="267,261,0,0" Click="btnCreateCustom_Click"/>
        <Button  x:Name="btnCreateRandom"   Content="{DynamicResource m_btnCreateRandom}"  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="444,261,0,0" Click="btnCreateRandom_Click"/>



    </Grid>
</Window>
